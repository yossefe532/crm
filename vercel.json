{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "cd web && npm install && npm run build",
  "outputDirectory": "web/.next",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://observant-achievement-production-bb94.up.railway.app/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/web/$1"
    }
  ]
}