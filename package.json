{
  "name": "real-estate-crm",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "UNLICENSED",
  "scripts": {
    "postinstall": "npx prisma generate",
    "dev": "npx ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "node node_modules/typescript/bin/tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev",
    "typecheck": "node node_modules/typescript/bin/tsc -p tsconfig.json --noEmit",
    "test": "vitest --run",
    "test:ports": "node scripts/run-port-tests.mjs",
    "owner:update": "node scripts/update-owner-credentials.mjs",
    "owner:test-login": "node scripts/test-owner-login.mjs",
    "rbac:seed": "node scripts/seed-rbac.mjs"
  },
  "dependencies": {
    "@prisma/client": "^5.11.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/web-push": "^3.6.4",
    "bcryptjs": "^3.0.3",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "prisma": "^5.11.0",
    "typescript": "^5.3.3",
    "web-push": "^3.6.7",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node-cron": "^3.0.7",
    "ts-node-dev": "^2.0.0",
    "vitest": "^1.6.0"
  }
}
